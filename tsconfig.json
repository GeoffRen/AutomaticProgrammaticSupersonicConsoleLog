{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "outDir": "out",
    "rootDir": "src",
    "sourceMap": true,
    "strict": false,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", ".vscode-test"]
}
